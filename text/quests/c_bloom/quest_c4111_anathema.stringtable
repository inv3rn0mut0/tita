    <Entry>
      <ID>5</ID>
      <DefaultText>Convinced Ioxu to Stop</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>Cultists killed Ioxu</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>Intimidated cultists</DefaultText>
      <FemaleText />
