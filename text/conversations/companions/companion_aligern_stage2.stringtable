      <FemaleText />
    </Entry>
    <Entry>
      <ID>282</ID>
      <DefaultText>"If that's what you want."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>283</ID>
      <DefaultText>"Do you know Rennio?"</DefaultText>
      <FemaleText />
    </Entry>
