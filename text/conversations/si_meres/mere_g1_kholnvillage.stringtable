      <FemaleText />
    </Entry>
    <Entry>
      <ID>1106</ID>
      <DefaultText>VILLAGE - PEOPLE PREPPING TO FIGHT
{DefensePlan 1}</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1107</ID>
      <DefaultText>VILLAGE - PEOPLE PREPPING TO EVAC{DefensePlan 2}</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1108</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1109</ID>
      <DefaultText>You are in the village center.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1110</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1111</ID>
      <DefaultText>Look around the village to get your bearings.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1112</ID>
      <DefaultText>Explore the village more thoroughly.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1113</ID>
      <DefaultText>EXPLORE MORE THOROUGHLY - BASE</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
