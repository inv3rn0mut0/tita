<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\a_sagus\a2501_explorer_from_steadfast</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>5</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"You aren't from around here, are you? Neither am I."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"I come from the Sea Kingdom of Ghan in the Steadfast. Far to the west, if you don't know it."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Never was much in favor of the sea. Thought I'd go inland, as far as I could. To see what I'd see."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"I've been on the road for... I don't know, a century or so? More?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Take care of yourself out there."</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
</StringTableFile>