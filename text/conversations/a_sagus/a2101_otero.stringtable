    <Entry>
      <ID>315</ID>
      <DefaultText>Rhin huffs quietly beside you. </DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>316</ID>
      <DefaultText>""</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>317</ID>
      <DefaultText>""</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>318</ID>
      <DefaultText>"If I see her, I'll try to bring her by."</DefaultText>
      <FemaleText />
