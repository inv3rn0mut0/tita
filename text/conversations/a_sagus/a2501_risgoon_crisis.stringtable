    <Entry>
      <ID>15</ID>
      <DefaultText>"Ris is down. Scatter!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>16</ID>
      <DefaultText>"We talking or fighting here?"</DefaultText>
      <FemaleText />
