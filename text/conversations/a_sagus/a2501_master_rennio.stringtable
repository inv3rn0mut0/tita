    <Entry>
      <ID>283</ID>
      <DefaultText>"You know, I had my doubts before, but now I know. You castoffs aren't human."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>284</ID>
      <DefaultText>"So long, filth."

</DefaultText>
      <FemaleText />
    </Entry>
