<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\a_sagus\a2501_old_sailor</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>5</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Not takin' to the air tonight, are yeh? It's a bad night for it."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"The wind comin' from the west is both hot and cold. The breath of dead machine demons, it's said."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Those who venture out into this starry night will never return, I warrant."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Horrors await careless sailors in the wide and hungry sky."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Be warned. Take sail tonight, and yeh'll never see Sagus again."</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
</StringTableFile>