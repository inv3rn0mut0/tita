    <Entry>
      <ID>435</ID>
      <DefaultText>"I was wondering what you're doing here."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>436</ID>
      <DefaultText>"What are you doing here again?"</DefaultText>
