    <Entry>
      <ID>139</ID>
      <DefaultText>"Careful. Careful."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>140</ID>
      <DefaultText>::critch::</DefaultText>
      <FemaleText />
