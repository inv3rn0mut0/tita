      <FemaleText />
    </Entry>
    <Entry>
      <ID>222</ID>
      <DefaultText>It bows. "Yes. For sticha friend, yes. Can take to egg tunnel. Then walk to place, yes?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>223</ID>
      <DefaultText>"We owe egg friend for help. Please not touch anything. We go."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>224</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>225</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>226</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>227</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>228</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>229</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>230</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>231</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
