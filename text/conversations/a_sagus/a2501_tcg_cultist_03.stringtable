<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\a_sagus\a2501_tcg_cultist_03</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>7</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Never trust a deceiver. Especially when they want you to *work*."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Anything... for you... holy one..."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Your holiness! You grace my life!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Tempt me not, deceiver! I won't turn away!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Greetings, brother!"</DefaultText>
      <FemaleText>"Greetings, sister!"</FemaleText>
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>"It is an honor to speak with one of the Children of the Changing God."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>"Please... speak with our elders in Circus Minor."</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
