      <ID>93</ID>
      <DefaultText>"Good. Tell me."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>94</ID>
      <DefaultText>"The mutant called Gh'zei."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>95</ID>
      <DefaultText>"The man named Mallet."</DefaultText>
      <FemaleText />
    </Entry>
