      <ID>38</ID>
      <DefaultText>Step into the darkness. </DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>39</ID>
      <DefaultText>Step into the darkness again.</DefaultText>
      <FemaleText />
    </Entry>
