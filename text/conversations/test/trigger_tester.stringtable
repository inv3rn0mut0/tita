  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Hi, I'm here to help you test out triggers. What would you like?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Have the neutral NPC walk into the trigger."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Right-o. There he goes!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>"Nothing for now, thanks."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>"Stop back if you need to test triggers!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>8</ID>
      <DefaultText>"Have the neutral NPC walk out of the trigger."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>9</ID>
      <DefaultText>"On it. Check him out!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>12</ID>
      <DefaultText>"Start a combat so I can test what happens when a hostile moves into the trigger."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>13</ID>
      <DefaultText>"Dangerous, but I like it! Done!"</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
